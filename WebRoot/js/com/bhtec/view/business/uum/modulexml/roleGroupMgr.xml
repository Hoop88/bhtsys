			<jsUrlShare>./ext/ux/multiselect/MultiSelect.js</jsUrlShare>
			<jsUrlShare>./js/com/bhtec/view/util/ux/MultiSelect.js</jsUrlShare>
			<jsUrl>./js/com/bhtec/view/business/uum/group/roleGroup/RoleGroupMember.js</jsUrl>
			<jsUrl>./js/com/bhtec/view/business/uum/group/roleGroup/RoleGroupVOp.js</jsUrl>
			<!-- 操作页面js类名 -->
			<jsMainClassName>com.bhtec.view.business.uum.group.roleGroup.RoleGroupVOp</jsMainClassName>
		</opView>

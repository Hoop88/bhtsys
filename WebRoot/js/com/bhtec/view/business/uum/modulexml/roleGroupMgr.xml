<?xml version="1.0" encoding="UTF-8"?>
<module>
	<dynamicLoadJs><!-- 动态加载js -->
		<listView><!-- 列表页面 -->
			<!-- js加载路径 -->
			<jsUrlShare>./ext/ux/grid/RowExpander.js</jsUrlShare>
			<jsUrl>./js/com/bhtec/view/business/uum/group/roleGroup/RoleGroupList.js</jsUrl>
			<!-- 列表js类名 -->
			<jsMainClassName>com.bhtec.view.business.uum.group.roleGroup.RoleGroupList</jsMainClassName>
		</listView>
		<opView><!-- 页面操作视图 jsUrlShare 全局共享-->
